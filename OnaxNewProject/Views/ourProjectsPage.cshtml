@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";
	//var tinafoxmainpic = Model.Value("aboutUsImage");
	//var riviewstars = Model.Value<IPublishedContent>("reviewsStarsImg");
	//var kevingardnermainpic = Model.Value<IPublishedContent>("reviewsMainImage");
	//var brandonminipic = Model.Value<IPublishedContent>("ourExpertsOneImg");
	//var ajcarterminipic = Model.Value<IPublishedContent>("ourExpertsTwoImg");
	//var aaroncarterminipic = Model.Value<IPublishedContent>("ourExpertsThreeImg");
	//var tinafoxminipic = Model.Value<IPublishedContent>("ourExpertsFourImg");
	//var brandonkerrminipic = Model.Value<IPublishedContent>("ourExpertsFiveImg");
	//var mirandadminipic = Model.Value<IPublishedContent>("ourExpertsSixImg");
	//var kevingardnerminipic = Model.Value<IPublishedContent>("ourExpertsSevenImg");
}

<div class="topmenu">
	<h1>@Model.Value("aboutUsHeadline")</h1>
</div>

<section class="aboutusmainpage">
	<div class="container">
		<div class="aboutusmainpage-content">
			<img class="tinafoxmainimage" src="" alt="Tina Fox">
			<p>@Model.Value("aboutUsUndertext")</p>
			<div class="aboutasidetext">
				<blockquote class="aboutusqoute">@Model.Value("aboutUsQuote")</blockquote>
				<p>@Model.Value("aboutUsDescription")</p>
			</div>
		</div>
	</div>
</section>
<section class="aboutustrustsection">
	<div class="container">
		<div class="aboutustrustsection-content">
			<h5>@Model.Value("reviewsTitle")</h5>
			<h2>@Model.Value("reviewsHeadline")</h2>
			<p class="reviewtext">@Model.Value("reviewsDescrption")</p>
			<img class="reviewstars" src="" alt="5 starts review">
			<p>@Model.Value("reviewsName")</p>
			<aside class="reviewsmainimg">
				<img class="kevingardnermainimg" src="" alt="">
			</aside>
		</div>
	</div>
</section>
<section class="aboutusourexpertsection">
	<div class="container">
		<div class="aboutusourexpertsection-content">
			<h5>@Model.Value("ourExpertsTitle")</h5>
			<h2>@Model.Value("ourExpertsHeadline")</h2>
			<div class="aboutusourexpertsectionimages1">
				<img class="brandonnpic" src="" alt="Brandon Newton">
				<h5>@Model.Value("ourExpertsOneName")</h5>
				<p>@Model.Value("ourExpertsOneTitle")</p>
				<img class="ajcarterpic" src="" alt="AJ Carter">
				<h5>@Model.Value("ourExpertsTwoName")</h5>
				<p>@Model.Value("ourExpertsTwoTitle")</p>
				<img class="aaronlambertpic" src="@aaroncarterminipic?.Url()" alt="Aaron Lambert">
				<h5>@Model.Value("ourExpertsThreeName")</h5>
				<p>@Model.Value("ourExpertsThreeTitle")</p>
			</div>
			<div class="aboutusourexpertsectionimages2">
				<img class="tinafoxpic" src="@tinafoxminipic?.Url()" alt="Tina Fox">
				<h5>@Model.Value("ourExpertsFourName")</h5>
				<p>@Model.Value("ourExpertsFourTitle")</p>
				<img class="brandonkerrpic" src="@brandonkerrminipic?.Url()" alt="Brandon Kerr">
				<h5>@Model.Value("ourExpertsFiveName")</h5>
				<p>@Model.Value("ourExpertsFiveTitle")</p>
				<img class="mirandadpic" src="@mirandadminipic?.Url()" alt="Miranda Dayton">
				<h5>@Model.Value("ourExpertsSixName")</h5>
				<p>@Model.Value("ourExpertsSixTitle")</p>
				<img class="kevingardnerpic" src="@kevingardnerminipic?.Url()" alt="Kevin Gardner">
				<h5>@Model.Value("ourExpertsSevenName")</h5>
				<p>@Model.Value("ourExpertsSevenTitle")</p>
			</div>
		</div>
	</div>
</section>
<section>@await Html.PartialAsync("letusknowpartialview")</section>
